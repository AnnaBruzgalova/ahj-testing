{
    "name": "testing",
    "version": "1.0.0",
    "description": "![CI](https://github.com/AnnaBruzgalova/ahj-testing/actions/workflows/web.yml/badge.svg)",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "lint": "eslint .",
        "test": "jest --testPathPattern=./src/",
        "coverage": "jest --coverage",
        "doc": "jsdoc -c jsdoc.conf.json",
        "preshow:coverage": "npm run coverage",
        "show:coverage": "live-server coverage/lcov-report",
        "preshow:doc": "npm run doc",
        "show:doc": "live-server doc",
        "preshow:dist": "npm run build",
        "show:dist": "live-server dist",
        "serve": "live-server ./src",
        "e2e": "jest --testPathPattern=./e2e/ --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnnaBruzgalova/ahj-testing.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AnnaBruzgalova/ahj-testing/issues"
    },
    "homepage": "https://github.com/AnnaBruzgalova/ahj-testing#readme",
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.3",
        "@babel/preset-env": "^7.20.2",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^5.0.0",
        "eslint": "^8.36.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "live-server": "^1.1.0",
        "mini-css-extract-plugin": "^2.7.5",
        "push-dir": "^0.4.1",
        "terser-webpack-plugin": "^5.3.7",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.1",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "babel-jest": "^29.5.0",
        "jest": "^29.5.0",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "puppeteer": "^19.8.0"
    }
}